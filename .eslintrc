{
   "env": {
      "node": true
   },
   "parser": "@typescript-eslint/parser",
   "plugins": ["@typescript-eslint"],
   "extends": [
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/recommended",
      "plugin:import/typescript"
   ],
   "rules": {
      "semi": ["error", "never"]
   }
}
